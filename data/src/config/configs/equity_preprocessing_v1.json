{
  "pipeline_id": "equity_preprocessing_v1",
  "description": "Comprehensive preprocessing pipeline for equity data with outlier handling and normalization",
  "asset_classes": [
    "equity",
    "etf"
  ],
  "rules": [
    {
      "rule_type": "missing_value_handling",
      "parameters": {
        "method": "forward_fill",
        "threshold": 0.05,
        "window_size": 5
      },
      "priority": 1
    },
    {
      "rule_type": "outlier_detection",
      "parameters": {
        "method": "iqr",
        "threshold": 2.5,
        "action": "winsorize"
      },
      "priority": 2
    },
    {
      "rule_type": "validation",
      "parameters": {
        "check_negative_prices": true,
        "check_zero_volume": true,
        "check_extreme_returns": true,
        "return_threshold": 0.5
      },
      "priority": 3
    },
    {
      "rule_type": "normalization",
      "parameters": {
        "method": "robust",
        "preserve_stats": true,
        "columns_to_normalize": [
          "open",
          "high",
          "low",
          "close",
          "volume"
        ]
      },
      "priority": 4
    }
  ],
  "quality_thresholds": {
    "completeness": 0.98,
    "consistency": 0.95,
    "accuracy": 0.9,
    "timeliness": 0.99,
    "uniqueness": 0.99,
    "min_data_points": 1000,
    "max_outlier_ratio": 0.05
  },
  "output_format": "parquet",
  "version": "1.0.0",
  "created_at": "2025-09-18T20:19:56.952500",
  "updated_at": "2025-09-18T20:19:56.952500",
  "metadata": {
    "created_by": "quant_system",
    "purpose": "portfolio_optimization",
    "data_sources": [
      "yahoo_finance"
    ],
    "expected_performance": "<30_seconds_for_1M_records"
  }
}